<Window x:Class="DeLightWPF.VideoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeLightWPF"
        mc:Ignorable="d"
        Title="VideoWindow" Height="450" Width="800">
    <Grid x:Name="grid">
        <MediaElement x:Name="VideoViewControl" LoadedBehavior="Manual" UnloadedBehavior="Manual" Stretch="UniformToFill"/>
    </Grid>
</Window>
